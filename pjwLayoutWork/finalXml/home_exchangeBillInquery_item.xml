<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tb_home_exchangeBillInquery_item_TopTb"
        android:layout_width="409dp"
        android:layout_height="10dp"
        android:background="@color/lightGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <LinearLayout
        android:id="@+id/layout_home_exchangeBillInquery_item_namePrintDetail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="6dp"
        app:layout_constraintTop_toBottomOf="@+id/tb_home_exchangeBillInquery_item_TopTb"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:textStyle="bold"
            android:text="원장번호가 여기에 출력됩니다 ." />

        <Button
            android:id="@+id/btn_home_exchangeBillInquery_item_printBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="@color/darkRed"
            android:text="출력" />

        <Button
            android:id="@+id/btn_home_exchange_item_bill_showDetail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="@color/dimGray"
            android:text="상세보기" />

    </LinearLayout>


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tb_home_exchangeBillInquery_item_underName"
        android:layout_width="409dp"
        android:layout_height="5dp"
        android:background="@color/lightGray"
        app:layout_constraintTop_toBottomOf="@+id/layout_home_exchangeBillInquery_item_namePrintDetail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <LinearLayout
        android:id="@+id/layout_home_exchangeBillInquery_item_keys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/tb_home_exchangeBillInquery_item_underName"
        app:layout_constraintStart_toStartOf="parent"
        android:padding="8dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_transactionDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:layout_margin="2dp"
            android:text="거래일자" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_paymentClassify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:layout_margin="2dp"
            android:text="수불구분" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_deliveryMethod"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="운송방법" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_publisher"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="발행인" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_summary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="적요" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_quantity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="수량" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_actualDestination"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="실배송처" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_salesAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="매출금액"/>

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemKey_depositAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="입금금액"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_home_exchangeBillInquery_item_values"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/tb_home_exchangeBillInquery_item_underName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/layout_home_exchangeBillInquery_item_keys"
        android:padding="8dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_transactionDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="00/00/00" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_paymentClassify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="수불구분 표시" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_deliveryMethod"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="운송방법 표시" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_publisher"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="발행인 이름 표시" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_summary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="매출요약 표시" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_quantity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="수량 표시" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_actualDestination"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="실배송처 표시" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_salesAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="매출금액 표시" />

        <TextView
            android:id="@+id/tv_home_exchangeBillInquery_itemValue_depositAmount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:textStyle="bold"
            android:text="입금금액 표시" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>